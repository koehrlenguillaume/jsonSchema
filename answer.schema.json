{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://raw.githubusercontent.com/koehrlenguillaume/jsonSchema/master/answer.schema.json",
  "title": "Answer",
  "description": "Prompt option answer",
  "type": "object",
  "properties": {
    "dialog": {
      "description": "Name of the dialog to redirect to. Ex: d_1_1_000",
      "type": "string"
    },
    "answer": {
      "description": "Answer value",
      "type": "string"
    }
  },
  "required": ["dialog", "answer"]
}
